library verilog;
use verilog.vl_types.all;
entity human_interface_corners is
    port(
        clk             : in     vl_logic;
        field           : in     vl_logic;
        left_button     : in     vl_logic;
        right_button    : in     vl_logic;
        up_button       : in     vl_logic;
        down_button     : in     vl_logic;
        enter_button    : in     vl_logic;
        zero_button     : in     vl_logic;
        one_button      : in     vl_logic;
        two_button      : in     vl_logic;
        three_button    : in     vl_logic;
        auto_corners    : in     vl_logic_vector(79 downto 0);
        set_corners     : in     vl_logic;
        corners1x       : out    vl_logic_vector(9 downto 0);
        corners1y       : out    vl_logic_vector(9 downto 0);
        corners2x       : out    vl_logic_vector(9 downto 0);
        corners2y       : out    vl_logic_vector(9 downto 0);
        corners3x       : out    vl_logic_vector(9 downto 0);
        corners3y       : out    vl_logic_vector(9 downto 0);
        corners4x       : out    vl_logic_vector(9 downto 0);
        corners4y       : out    vl_logic_vector(9 downto 0)
    );
end human_interface_corners;
